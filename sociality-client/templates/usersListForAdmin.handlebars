<div id="main-content" class="container">
    <div class="row row-main">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Users List</h3></div>
            <div class="panel-body">
                <table class="table">
                    <thead>
                    <tr>
                        <th>Username</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Role</th>
                        <th>Created At</th>
                        <th>Enabled</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each this}}
                        <tr>
                            <td>{{username}}</td>
                            <td>{{firstName}}</td>
                            <td>{{lastName}}</td>
                            <td>{{role}}</td>
                            <td>{{createdAt.date}}</td>
                            <td>{{enabled}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>